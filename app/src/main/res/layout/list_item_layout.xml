<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:padding="16dp">

    <!-- Example TextView to display a field from YourResponseModel -->
    <TextView
        android:id="@+id/text_view_layout_no"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Result: "
        android:textSize="18sp"
        android:textStyle="bold" />

    <!-- Example TextView to display another field from YourResponseModel -->
    <TextView
        android:id="@+id/TvResMsg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Response Msg "
        android:textSize="16sp" />

    <!-- Add more TextViews or other UI elements as needed to display your data -->

</LinearLayout>
